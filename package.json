{
  "name": "agency-landing-page-with-tailwind",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@tailwindcss/cli": "^4.1.15",
    "tailwindcss": "^4.1.15"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1"
  },
  "scripts": {
    "prebuild": "chmod +x ./node_modules/.bin/tailwindcss || true",
    "dev": "tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "build": "tailwindcss -i ./src/input.css -o ./src/output.css -m"
  }
}
